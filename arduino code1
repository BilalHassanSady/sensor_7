
void setup()
{
  pinMode(6, OUTPUT);
    pinMode(8, OUTPUT);
    pinMode(4, OUTPUT);
    pinMode(3, OUTPUT);
}

void loop()
{
  digitalWrite(6, HIGH);
  digitalWrite(8, LOW);
  digitalWrite(4, HIGH);
  digitalWrite(3, LOW);
  delay(5000); 
  digitalWrite(6, LOW);
    digitalWrite(8, HIGH);
    digitalWrite(4, LOW);
  digitalWrite(3, HIGH);
  delay(5000);
}
